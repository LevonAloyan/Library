<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <welcome-file-list>
        <welcome-file>/WEB-INF/login.jsp</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>registerPage</servlet-name>
        <jsp-file>/WEB-INF/register.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>registerPage</servlet-name>
        <url-pattern>/registerPage</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>unAssign</servlet-name>
        <jsp-file>/WEB-INF/unAssign.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>unAssign</servlet-name>
        <url-pattern>/unassigned</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>dashboard</servlet-name>
        <jsp-file>/WEB-INF/userDashboard.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>dashboard</servlet-name>
        <url-pattern>/dashboard</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>error403</servlet-name>
        <jsp-file>/WEB-INF/error_403.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>error403</servlet-name>
        <url-pattern>/error_403</url-pattern>
    </servlet-mapping>


